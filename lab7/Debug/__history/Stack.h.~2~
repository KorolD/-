#ifndef StackH
#define StackH Stack

#include <string.h>

struct Item{
	struct ItemStack* next;
	int key;
	String data;
};

class ItemStack{
	public:
	ItemStack(){}
	~ItemStack(){}
	struct Item* FindKey(int k){}
	void DeleteKey(int key){}
	void Add(int key, String data){}
	String Show(){}
	int LessThanOrEq(double average){}
	double Average(){}
	bool IsEmpty(){}
	int GetAmount(){}

	private:
	struct ItemStack* stack_ptr;
   //	DISALLOW_COPY_AND_ASSIGN(ItemStack);
};

#endif
